{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "clean": "rimraf dist/win-unpacked",
    "build:main": "tsc --project tsconfig.electron.json",
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "npm run clean && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "package": "electron-builder",
    "build:all": "npm run build && npm run package"
  },
  "main": "dist-electron/main.js",
  "build": {
    "appId": "com.example.myapp",
    "files": [
      "dist-electron/**/*",
      "dist/**/*",
      "package.json"
    ]
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-electron": "^0.29.0",
    "wait-on": "^8.0.2"
  }
}
